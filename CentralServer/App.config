<?xml version="1.0" encoding="utf-8" ?>
<configuration>

	<connectionStrings>
		<add name="Default" connectionString="Data Source=.\CentralServerDB.db;Version=3;" providerName="System.Data.SqlClient"/>
	</connectionStrings>
	
	<appSettings>
		
		<add key="CentralServerPort" value ="34258"/>								<!--int-->
		
		<!--Logger section-->
		<add key="UseAsynchronousLogging" value="True"/>		<!--bool-->
		<add key="SizeLimitInMB" value="10"/>					<!--int-->
		<add key="EnableLogging" value="True"/>					<!--bool-->
		<add key="LoggingDirectory" value ="C:\Logs"/>			<!--string-->
		<add key="BufferSize" value="10"/>						<!--int-->
		
	</appSettings>
	
</configuration>